{
  "name": "jolks.github.io",
  "version": "1.0.0",
  "description": "* sandbox - contains misc test code.",
  "main": "index.html",
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "hexo-cli": "^3.1.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "site:init": "hexo init .hexo",
    "site:clean": "hexo --cwd .hexo clean",
    "site:generate": "hexo --cwd .hexo generate",
    "site:generate:watch": "hexo --cwd .hexo generate --watch",
    "site:dev": "hexo --cwd .hexo server --log",
    "site:deploy": "cp -rf .hexo/public/* ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jolks/jolks.github.io.git"
  },
